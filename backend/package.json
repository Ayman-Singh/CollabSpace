{
  "name": "collabspace-backend",
  "version": "1.0.0",
  "description": "CollabSpace Backend API - Production-grade real-time collaboration platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:seed": "ts-node src/database/seed.ts"
  },
  "keywords": [
    "collaboration",
    "real-time",
    "websockets",
    "api",
    "typescript",
    "nodejs"
  ],
  "author": "Ayman Singh",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-github2": "^0.1.12",
    "socket.io": "^4.7.4",
    "socket.io-redis": "^6.1.1",
    "redis": "^4.6.10",
    "prisma": "^5.6.0",
    "@prisma/client": "^5.6.0",
    "postgresql": "^0.0.1",
    "pg": "^8.11.3",
    "joi": "^17.11.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "aws-sdk": "^2.1499.0",
    "nodemailer": "^6.9.7",
    "twilio": "^4.19.0",
    "winston": "^3.11.0",
    "prometheus-client": "^0.5.1",
    "express-prometheus-middleware": "^1.2.1",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "axios": "^1.6.2",
